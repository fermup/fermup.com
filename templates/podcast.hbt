{{> header}}

<article>
  {{{contents}}}
</article>

<div class="podcast-list">
  <ul>
    {{#each collections.podcast}}
      <li>
        <a href="/podcast/{{this.title}}/">
          <h2>{{this.title}}: {{this.titlePart}}</h2>
          <article>{{this.summary}}</article>
        </a>
      </li>
    {{/each}}
  </ul>
</div>

{{> footer}}
